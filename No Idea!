n, m = map(int, input().split())
array = list(map(int, input().split()))
setA = set(list(map(int, input().split())))
setB = set(list(map(int, input().split())))
happiness = 0
for a in array:
    if(a in setA):
        happiness+=1

for a in array:
    if(a in setB):
        happiness-=1
print(happiness)
